{
  "name": "@rebass/grid",
  "version": "6.0.0-6",
  "description": "Responsive React grid system built with styled-system, with support for styled-components and emotion",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "rm -rf dist && babel src -d dist && npm run emotion",
    "test": "jest",
    "start": "mdx-go docs",
    "build": "mdx-go build docs -d site",
    "emotion": "NODE_ENV=emotion babel src -d dist/emotion",
    "logo": "repng docs/logo.js -w 768 -h 768 -d docs -f logo.png"
  },
  "keywords": [
    "react",
    "styled-components",
    "styled-system",
    "grid",
    "flexbox",
    "layout",
    "css"
  ],
  "author": "Brent Jackson",
  "license": "MIT",
  "dependencies": {
    "styled-system": "^3.1.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@compositor/kit": "^1.0.47",
    "@emotion/core": "^10.0.1",
    "@emotion/styled": "^10.0.1",
    "@reach/component-component": "^0.1.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "jest": "^23.6.0",
    "jest-emotion": "^10.0.1",
    "jest-styled-components": "^6.3.1",
    "mdx-go": "^2.0.0-32",
    "react-live": "^1.12.0",
    "react-test-renderer": "^16.6.3",
    "reline": "^1.0.0-beta.3",
    "repng": "^3.1.0",
    "styled-components": "^4.1.2"
  },
  "jest": {
    "roots": [
      "<rootDir>/tests/"
    ],
    "testMatch": [
      "**/tests/**/*.js"
    ]
  },
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rebassjs/grid.git"
  },
  "bugs": {
    "url": "https://github.com/rebassjs/grid/issues"
  },
  "homepage": "https://github.com/rebassjs/grid#readme"
}
