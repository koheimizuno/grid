{
  "name": "@rebass/grid",
  "version": "6.0.0-2",
  "description": "Responsive React grid system built with styled-system, with support for styled-components and emotion",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "rm -rf dist && babel src -d dist && npm run emotion",
    "test": "jest",
    "start": "mdx-go docs",
    "build": "mdx-go build docs -d site --basename /grid",
    "emotion": "NODE_ENV=emotion babel src -d dist/emotion",
    "logo": "repng docs/logo.js -w 768 -h 768 -d docs -f logo.png"
  },
  "keywords": [
    "react",
    "styled-components",
    "styled-system",
    "grid",
    "flexbox",
    "layout",
    "css"
  ],
  "author": "Brent Jackson",
  "license": "MIT",
  "dependencies": {
    "styled-system": "^3.1.3"
  },
  "devDependencies": {
    "@compositor/kit": "^1.0.47",
    "@reach/component-component": "^0.1.1",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "emotion": "^9.2.8",
    "jest": "^23.6.0",
    "jest-emotion": "^9.2.7",
    "jest-styled-components": "^6.2.0",
    "mdx-go": "^1.1.6",
    "react-emotion": "^9.2.8",
    "react-live": "^1.11.0",
    "react-test-renderer": "^16.5.0",
    "reline": "^1.0.0-beta.3",
    "repng": "^3.1.0",
    "styled-components": "^4.0.0-beta.3-side-effect-test"
  },
  "peerDependencies": {
    "styled-components": ">=2.0",
    "emotion": ">=9.0",
    "react-emotion": ">=9.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/tests/"
    ],
    "testMatch": [
      "**/tests/**/*.js"
    ]
  }
}
